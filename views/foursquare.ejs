<% include header.ejs %>

<% include navbar.ejs %>

<% include foursquare/checkinsurplus.ejs %>

<h2>Recent Checkins</h2>
<table id="checkins-table">
<% for(var i = 0; i < checkinsData.response.checkins.items.length; i++) {%>
    <tr>
	<td><%= checkinsData.response.checkins.items[i].venue.name %></td>
	<td><%= checkinsData.response.checkins.items[i].venue.location.address %></td>
        <% if (checkinsData.response.checkins.items[i].venue.categories[0]) { %>
            <td><%= checkinsData.response.checkins.items[i].venue.categories[0].name %></td>
        <% } %>    
    </tr>
<% } %>
</table>
<% include footer.ejs %>
